<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="consulta-task-flow">
    <default-activity>Main</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="Main">
      <page>/Main.jsff</page>
      <train-stop/>
    </view>
    <view id="Consulta">
      <page>/Consulta.jsff</page>
      <train-stop/>
    </view>
    <view id="editor">
      <page>/editor.jsff</page>
      <train-stop/>
    </view>
    <train/>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
